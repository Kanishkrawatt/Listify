{
  "name": "Listify",
  "icons": {
    "16": "public/icon-16.png",
    "48": "public/icon-48.png",
    "128": "public/icon-48.png"
  },
  "version": "1.0",
  "description": "A simple ToDo App, built with javascript, html and css",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "contextMenus",
    "scripting",
    "tabs",
    "nativeMessaging"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "public/icon-48.png"
  },
  "manifest_version": 3
}
